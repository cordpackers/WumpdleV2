	},
	"apiBases": {
		"v1": "https://discord.com/api",
		"v2": "https://discord.com/api/updates"
	},
	"webhook": {
		"enable": false,
		"url": "https://discord.com/api/webhooks/X",
		"username": "GooseUpdate",
		"avatarUrl": "https://cdn.discordapp.com/avatars/760559484342501406/5125aff2f446ad7c45cf2dfd6abf92ed.png"
	}
